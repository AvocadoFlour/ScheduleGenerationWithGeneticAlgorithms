<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="vocationsTabVBox" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.JavaFX.vocations.VocationsTabController">
   <children>
      <TableView fx:id="vocationsTableView">
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <HBox fx:id="vocationsTabHBox" prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
         <children>
            <Button fx:id="deleteVocationButton" mnemonicParsing="false" text="Delete Vocation" />
            <Button fx:id="createVocationButton" mnemonicParsing="false" text="Create vocation" HBox.hgrow="ALWAYS" />
            <Button fx:id="vocationDetailsButton" mnemonicParsing="false" text="Show details" />
         </children>
      </HBox>
   </children>
</VBox>
