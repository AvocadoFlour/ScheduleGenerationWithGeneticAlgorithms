<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<VBox fx:id="lecturersVBox" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.JavaFX.lecturers.LecturersTabController">
   <children>
        <TableView fx:id="lecturersTableView" prefHeight="151.0" prefWidth="241.0" />
      <HBox fx:id="lecturersHBox" prefHeight="100.0" prefWidth="200.0">
         <children>
                <Button fx:id="deleteLecturerButton" mnemonicParsing="false" text="Delete selected" />
                <Button fx:id="createNewLecturerButton" mnemonicParsing="false" onAction="#openLecturerInputWindow" text="Add new lecturer" />
         </children>
      </HBox>
   </children>
</VBox>
