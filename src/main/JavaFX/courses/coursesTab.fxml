<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<SplitPane fx:id="coursesSplitPane" dividerPositions="0.5" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.JavaFX.courses.CoursesTabController">
    <items>
        <TableView fx:id="coursesTableView" prefHeight="151.0" prefWidth="241.0" />
      <VBox fx:id="coursesVBox">
         <children>
                <Button fx:id="createNewCourseButton" mnemonicParsing="false" onAction="#openCoursesInputWindow" text="Add new course" />
                <Button fx:id="deleteLCourseButton" mnemonicParsing="false" text="Delete selected" />
         </children>
      </VBox>
    </items>
</SplitPane>
