<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<SplitPane dividerPositions="0.5" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.JavaFX.courses.CoursesTabController">
    <items>
        <TableView fx:id="coursesTableView" prefHeight="151.0" prefWidth="241.0" />
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
            <children>
                <Button fx:id="deleteLCourseButton" layoutX="100.0" layoutY="89.0" mnemonicParsing="false" text="Delete selected" />
                <Button fx:id="addNewCourseButton" layoutX="98.0" layoutY="48.0" mnemonicParsing="false" onAction="#openCoursesInputWindow" text="Add new course" />
            </children>
        </AnchorPane>
    </items>
</SplitPane>
