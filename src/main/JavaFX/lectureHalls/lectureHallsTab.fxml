<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<VBox fx:id="lectureHallsVBox" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.JavaFX.lectureHalls.LectureHallsTabController">
   <children>
        <TableView fx:id="lectureHallsTableView" prefHeight="151.0" prefWidth="241.0" />
      <HBox fx:id="lectureHallsHBox" prefHeight="100.0" prefWidth="200.0">
         <children>
                <Button fx:id="deleteLectureHallButton" mnemonicParsing="false" text="Delete selected" />
                <Button fx:id="createNewLectureHallButton" mnemonicParsing="false" onAction="#openLectureHallInputWindow" text="Add new Lecture hall" />
         </children>
      </HBox>
   </children>
</VBox>
