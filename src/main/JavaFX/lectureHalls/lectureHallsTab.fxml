<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<SplitPane dividerPositions="0.5" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/15.0.1" fx:controller="main.JavaFX.lectureHalls.LectureHallsTabController">
    <items>
        <TableView fx:id="lectureHallsTableView" prefHeight="151.0" prefWidth="241.0" />
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
            <children>
                <Button fx:id="deleteLectureHallButton" layoutX="100.0" layoutY="89.0" mnemonicParsing="false" text="Delete selected" />
                <Button fx:id="addNewLectureHallButton" layoutX="85.0" layoutY="48.0" mnemonicParsing="false" onAction="#openLectureHallInputWindow" text="Add new Lecture hall" />
            </children>
        </AnchorPane>
    </items>
</SplitPane>
